{
  "manifest_version": 2,
  "name": "Web Page Icon Status",
  "version": "0.0.0.2",
  "description": "This extension is a easy way to set an icon to a page.",
  "icons": {
    "16": "images/icons.iconarchive.com/icons/paomedia/small-n-flat/16/tag-alt-icon.png",
    "24": "images/icons.iconarchive.com/icons/paomedia/small-n-flat/24/tag-alt-icon.png",
    "32": "images/icons.iconarchive.com/icons/paomedia/small-n-flat/32/tag-alt-icon.png",
    "48": "images/icons.iconarchive.com/icons/paomedia/small-n-flat/48/tag-alt-icon.png",
    "64": "images/icons.iconarchive.com/icons/paomedia/small-n-flat/64/tag-alt-icon.png",
    "72": "images/icons.iconarchive.com/icons/paomedia/small-n-flat/72/tag-alt-icon.png",
    "96": "images/icons.iconarchive.com/icons/paomedia/small-n-flat/96/tag-alt-icon.png",
    "128": "images/icons.iconarchive.com/icons/paomedia/small-n-flat/128/tag-alt-icon.png",
    "512": "images/icons.iconarchive.com/icons/paomedia/small-n-flat/512/tag-alt-icon.png",
    "1024": "images/icons.iconarchive.com/icons/paomedia/small-n-flat/1024/tag-alt-icon.png"
  },
  "author": "Abbas Gussenov",
  "page_action": {
    "default_icon": {
      "16": "images/icons.iconarchive.com/icons/pixelmixer/basic/16/label-icon.png",
      "32": "images/icons.iconarchive.com/icons/pixelmixer/basic/32/label-icon.png",
      "48": "images/icons.iconarchive.com/icons/pixelmixer/basic/48/label-icon.png",
      "64": "images/icons.iconarchive.com/icons/pixelmixer/basic/64/label-icon.png"
    },
    "default_popup": "icons.html",
    "default_title": "Set an Icon"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "short_name": "WebPageIconStatus",
  "background": {
    "page": "background.html"
  }
}
